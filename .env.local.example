# Local Development Environment Variables
# Copy this file to .env.local and adjust as needed
#
# Note: The default docker-compose.yml uses hardcoded values for local development.
# These environment variables are only needed if you want to override the defaults
# or run services outside of Docker.

# Database connection
DATABASE_URL=postgresql://dabb:dabb_dev_password@localhost:5432/dabb

# Server configuration
NODE_ENV=development
PORT=3000
CLIENT_URL=http://localhost:5173

# Web app configuration (build-time)
VITE_SERVER_URL=http://localhost:3000

# PostgreSQL (used by docker-compose)
POSTGRES_USER=dabb
POSTGRES_PASSWORD=dabb_dev_password
POSTGRES_DB=dabb
