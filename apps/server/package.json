{
  "name": "@dabb/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc && cp -r src/db/migrations dist/db/",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist coverage",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "tsx src/db/runMigrations.standalone.ts"
  },
  "dependencies": {
    "@dabb/game-logic": "workspace:*",
    "@dabb/shared-types": "workspace:*",
    "cors": "^2.8.5",
    "envalid": "^8.1.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "pg": "^8.18.0",
    "postgres-migrations": "^5.3.0",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "rate-limiter-flexible": "^9.1.1",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^25.2.1",
    "@types/pg": "^8.11.10",
    "@vitest/coverage-v8": "^4.0.18",
    "socket.io-client": "^4.8.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
